.announcementManagementDeTopC ul li:hover{ color: #f37427;}
.announcementManagementDeTop>a{ display: inline-block; float: right; margin-top: 30px; margin-right: 30px; width: 120px; height: 36px; border-radius: 3px; background: #f37526;}
.announcementManagementDeTop>a:hover{ background: #ff8138;}
.announcementManagementDeTop>a span{ display: inline-block; font-size: 14px; color: #ffffff; height: 36px; line-height: 36px; padding-right: 21px; background: url(../images/myPic14.png) right center no-repeat;}
.announcementManagementDeBodyT{ margin-left: 30px; margin-top: 20px; width: 960px; height: 50px; background: #f7f7f7;}
