.announcementManagementTop img{ float: left; margin: 15px 12px 0;}
.announcementManagementDe{ padding-bottom: 20px; background: #ffffff; margin-top: 20px;}
.announcementManagementDeTopL{ float: left; margin-left: 30px; margin-top: 30px; width: 235px; height: 34px; border: 1px solid #cccccc; border-radius: 4px; background: #f5f5f5; position: relative; cursor: pointer;}
.announcementManagementDeTopL:hover{ background: #fff;}
