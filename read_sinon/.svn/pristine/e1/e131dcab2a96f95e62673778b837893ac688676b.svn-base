.announcementManagementDeBodyB{ width: 960px; margin-left: 30px; text-align: left;}
.announcementManagementDeBodyB ul li{ width: 100%; height: 50px; background: #ffffff;}
.announcementManagementDeBodyB ul li:hover{ background: #faf3ef;}
.announcementManagementDeBodyB ul li>div{ float: left; font-size: 12px; color: #666666; height: 50px; line-height: 50px; position: relative;}
.announcementManagementDeBodyB ul li>div p{ float: left; width: 225px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.announcementManagementDeBodyB ul li>div span{ display: inline-block; float: left; margin-left: 10px; color: #288df0; cursor: pointer;}
.announcementManagementDeBodyB ul li>div i{ display: inline-block; font-style: normal; float: left; padding-right: 14px; background: url(../images/myPic15.png) right center no-repeat; cursor: pointer;}
.announcementManagementDeBodyB ul li>div i:hover{ color: #f37427; background: url(../images/myPic16.png) right center no-repeat;}
.announcementManagementDeBodyB ul li>div div{ display: none; line-height: 30px; width: 100px; padding: 10px 0; background: #ffffff; position: absolute; top: 33px; left: 0; z-index: 10; border: 1px solid #e7e7e7;}
.announcementManagementDeBodyB ul li>div div a{ display: inline-block; font-size: 12px; color: #666666; height: 30px; line-height: 30px; width: 100%; text-align: center; cursor: pointer;}
.announcementManagementDeBodyB ul li>div div a:hover{ background: #f37526; color: #ffffff;}
