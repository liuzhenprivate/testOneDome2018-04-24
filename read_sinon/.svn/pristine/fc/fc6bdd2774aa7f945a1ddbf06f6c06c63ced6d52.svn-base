/*榜单配置（编辑书籍）排序*/
.sortAlert{ width: 560px; padding-bottom: 12px; background: #f7f7f7;}
.sortDe{ width: 100%; height: 96px; background: #ffffff;}
.sortDe span{ display: inline-block; font-size: 12px; color: #666666; height: 36px; line-height: 36px; float: left; margin-left: 38px; margin-top: 30px;}
.sortDe div{ width: 408px; height: 34px; border: 1px solid #cccccc; float: left; margin-left: 15px; margin-top: 30px; position: relative;}
.sortDe div input{ font-size: 12px; color: #333; height: 18px; line-height: 18px; float: left; margin-left: 11px; margin-top: 8px; width: 300px;}
.sortDe div i{ display: inline-block; font-style: normal; font-size: 12px; color: #999999; height: 34px; line-height: 34px; float: right; margin-right: 10px;}
.sortDe div p{ font-size: 12px; color: #cccccc; height: 34px; line-height: 34px; position: absolute; text-align: left; left: 0; top: 0; padding-left: 11px; width: 305px;}
.sortBtn{ font-size: 14px; color: #ffffff; height: 36px; line-height: 36px; width: 120px; border-radius: 3px; background: #f17526; margin: 12px auto 0; cursor: pointer;}

/*榜单配置（编辑书籍）添加书籍*/
.addBooks{ width: 670px; padding-bottom: 12px; background: #f7f7f7;}
.addBooksTop{ padding-top: 10px; margin-left: 29px;}
.addBooksTop span{ display: inline-block; font-size: 14px; color: #333333; height: 54px; line-height: 54px; float: left;}
.addBooksTop p{ font-size: 12px; color: #999999; height: 54px; line-height: 54px; float: right; margin-right: 29px;}
.addBooksSelL{ float: left; margin-left: 30px; width: 228px; height: 34px; border: 1px solid #cccccc; position: relative;}
.addBooksSelL input{ font-size: 12px; color: #323232; height: 20px; line-height: 20px; float: left; margin-left: 10px; margin-top: 7px; width: 180px;}
.addBooksSelL p{ font-size: 12px; color: #cccccc; height: 34px; line-height: 34px; position: absolute; left: 0; top: 0; padding-left: 10px; width: 185px; text-align: left;}
.addBooksSelL img{ width: 12px; float: right; margin-right: 10px; margin-top: 11px; cursor: pointer;}
.addBooksSelC{ float: left; margin-left: 10px; width: 115px; height: 34px; border: 1px solid #cccccc; border-radius: 4px; background: #f5f5f5; position: relative; cursor: pointer;}
.addBooksSelC:hover{ background: #fff;}
.addBooksSelC p{ font-size: 14px; color: #666666; height: 34px; line-height: 34px; float: left; margin-left: 20px;}
.addBooksSelC img{ float: right; margin-top: 14px; margin-right: 20px;}
.addBooksSelC ul{ display: none; width: 100%; border: 1px solid #cccccc; position: absolute; left: -1px; top: 34px; background: #fff; padding: 10px 0 7px 0; z-index: 5;}
.addBooksSelC ul li{ text-align: left; font-size: 12px; color: #666666; height: 30px; line-height: 30px; padding-left: 20px;}
.addBooksSelC ul li:hover{ color: #f37427;}
.addBooksSelTit{ width: 598px; height: 50px; margin-left: 30px; margin-top: 20px; background: #f7f7f7;}
.addBooksSelTit span{ display: block; width: 10px; height: 10px; border: 1px solid #cccccc; float: left; margin-left: 20px; margin-top: 19px; background: #ffffff;}
.addBooksSelTit span i{ display: block; width: 10px; height: 10px;}
.addBooksSelTit span i.addBooksSelTitAct{ background: url(../images/myPic35.png) center center no-repeat;}
.addBooksSelTit div{ font-size: 14px; color: #333333; height: 50px; line-height: 50px; float: left; text-align: left;}
.addBooksSelTex{ width: 598px; margin-left: 30px; text-align: left; height: 360px; overflow-y: scroll;}
.addBooksSelTex ul li{ width: 100%; height: 120px; position: relative;}
.addBooksSelTex ul li>div{ float: left;}
.addBooksSelTex ul li a{ display: block; width: 10px; height: 10px; border: 1px solid #cccccc; float: left; background: #ffffff; margin-left: 20px; margin-top: 54px;}
.addBooksSelTex ul li a span{ display: block; width: 10px; height: 10px;}
.addBooksSelTex ul li a span.addBooksSelTexAct{ background: url(../images/myPic35.png) center center no-repeat;}
.addBooksSelTexFo,.addBooksSelTexFi,.addBooksSelTexSi,.addBooksSelTexSe{ font-size: 12px; color: #666666; height: 120px; line-height: 120px;}
.addBooksSelTexTw{ padding-top: 20px; width: 70px; margin-left: 21px;}
.addBooksSelTexTw img{ display: block; width: 60px; height: 80px;}
.addBooksSelTexTh{ padding-top: 20px; width: 146px;}
.addBooksSelTexTh p{ font-size: 12px; color: #666666; height: 20px; line-height: 20px; width: 130px; text-overflow: ellipsis; white-space: nowrap; overflow: hidden;}
.addBooksSelTexTh p i{ font-style: normal; color: #999999;}
.addBooksSelTexFo{ width: 103px;}
.addBooksSelTexFi{ width: 89px;}
.addBooksSelTexFi p i{ font-style: normal; color: #f37427;}
.addBooksSelTexSi{ width: 75px;}

/*页面配置（专题模块）选择链接*/
.selectLink{ width: 560px; padding-bottom: 12px; background: #f7f7f7;}
.selectLinkDe{ padding: 30px 0; background: #ffffff;}
.selectLinkDe ul{ margin-left: 30px; height: 200px; overflow-y: scroll;}
.selectLinkDe ul li{ height: 50px; width: 490px;}
.selectLinkDe ul li img{ float: left; margin-top: 17px; margin-left: 20px;}
.selectLinkDe ul li span{ display: inline-block; font-size: 14px; color: #666666; height: 50px; line-height: 50px; float: left; margin-left: 20px;}
.selectLinkDe ul li a{ display: inline-block; font-size: 14px; color: #666666; height: 50px; line-height: 50px; float: left; margin-left: 31px;}
.selectLinkBtn{ font-size: 14px; color: #ffffff; height: 36px; line-height: 36px; width: 120px; border-radius: 3px; background: #f17526; margin: 12px auto 0; cursor: pointer;}














































