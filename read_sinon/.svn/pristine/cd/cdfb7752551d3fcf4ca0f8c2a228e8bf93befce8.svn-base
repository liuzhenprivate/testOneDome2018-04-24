<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ExtensionRecordMapper">
	
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into TB_EXTENSION_RECORD(
			EXTENSION_CONNECT_ID,	
			USERID,	
			TYPE,	
			RECHARGE,	
			STATE,	
			CREATE_TIME,	
			OVER_TIME,	
			CHANNEL,	
			IP
		) values (
			#{EXTENSION_CONNECT_ID},	
			#{USERID},	
			#{TYPE},	
			#{RECHARGE},	
			#{STATE},	
			#{CREATE_TIME},	
			#{OVER_TIME},	
			#{CHANNEL},	
			#{IP}
		)
	</insert>
	
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from TB_EXTENSION_RECORD
		where 
			EXTENSION_RECORD_ID = #{EXTENSION_RECORD_ID}
	</delete>
	
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update  TB_EXTENSION_RECORD
			set 
				EXTENSION_CONNECT_ID = #{EXTENSION_CONNECT_ID},
				USERID = #{USERID},
				TYPE = #{TYPE},
				RECHARGE = #{RECHARGE},
				STATE = #{STATE},
				CREATE_TIME = #{CREATE_TIME},
				OVER_TIME = #{OVER_TIME},
				CHANNEL = #{CHANNEL},
				IP = #{IP}
			where 
				EXTENSION_RECORD_ID = #{EXTENSION_RECORD_ID}
	</update>
	
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
			EXTENSION_RECORD_ID,	
			EXTENSION_CONNECT_ID,	
			USERID,	
			TYPE,	
			RECHARGE,	
			STATE,	
			CREATE_TIME,	
			OVER_TIME,	
			CHANNEL,	
			IP
		from 
			TB_EXTENSION_RECORD
		where 
			EXTENSION_RECORD_ID = #{EXTENSION_RECORD_ID}
	</select>
	
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
				a.EXTENSION_RECORD_ID,	
				a.EXTENSION_CONNECT_ID,	
				a.USERID,	
				a.TYPE,	
				a.RECHARGE,	
				a.STATE,	
				a.CREATE_TIME,	
				a.OVER_TIME,	
				a.CHANNEL,	
				a.IP
		from 
				TB_EXTENSION_RECORD a
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
				a.EXTENSION_RECORD_ID,	
				a.EXTENSION_CONNECT_ID,	
				a.USERID,	
				a.TYPE,	
				a.RECHARGE,	
				a.STATE,	
				a.CREATE_TIME,	
				a.OVER_TIME,	
				a.CHANNEL,	
				a.IP
		from 
				TB_EXTENSION_RECORD a
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from TB_EXTENSION_RECORD
		where 
			EXTENSION_RECORD_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
</mapper>