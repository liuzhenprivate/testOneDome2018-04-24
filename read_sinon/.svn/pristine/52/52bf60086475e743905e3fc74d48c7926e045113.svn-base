	<script>
		$('.libraryEditeInformationDePic ul li').click(function(){
			if($(this).hasClass('libraryEditeInformationDePicAct')){
				$(this).removeClass('libraryEditeInformationDePicAct');
				$(this).find('.libraryEditeInformationDePic2').attr('src','../images/myPic56.png');
			}else{
				$(this).addClass('libraryEditeInformationDePicAct');
				$(this).find('.libraryEditeInformationDePic2').attr('src','../images/myPic55.png');
			}
		})
		
		$('.libraryEditeInformationDeTop2').click(function(){
			$('.libraryEditeInformationDePic ul li.libraryEditeInformationDePicAct').remove();
		})
		
		$('.libraryEditeInformationDeTop1').click(function(){
			$('.changeClassifiAlert', parent.document).show();
			(function(Thunder){
				$('.changeClassifiAlert', parent.document).css('padding-top',Thunder);
			})((parent.document.documentElement.clientHeight-$('.changeClassifiDe', parent.document).get(0).offsetHeight)/2);
		})
	</script>
