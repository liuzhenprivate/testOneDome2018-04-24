<?xml version="1.0" encoding="utf-8"?>  
    <!DOCTYPE urlrewrite  
    PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"  
    "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">  
<urlrewrite>  
  
    <!-- 不带参数的重写URL -->  
    <rule>  
        <from>(.*)/test.html</from>  
        <to>$1/TestServlet1</to>  
    </rule>  
   <rule>  
        <from>(.*)/userinf-(\w*).html</from>  
        <to>$1/recharge/rechargehtml/$2</to>  
    </rule> 
    <!-- 带参数的重写URL   
        Id=$2 ,.*代表所有传人的参数.  
    -->  
   
  
</urlrewrite>  